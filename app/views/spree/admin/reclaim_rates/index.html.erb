<%= render 'spree/admin/shared/accounts_accordion' %>

<div id="acc_content">

<table>
  <tr>
    <th>Reclaim rate</th>
    <th></th>
    <th></th>
  </tr>

<% @admin_reclaim_rates.each do |admin_reclaim_rate| %>
  <tr>
    <td><%= admin_reclaim_rate.reclaim_rate %></td>
    <td><%= link_to 'Edit', edit_admin_reclaim_rate_path(admin_reclaim_rate) %></td>
    <td><%= link_to 'Destroy', admin_reclaim_rate, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Reclaim rate', new_admin_reclaim_rate_path %>
</div>